local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local CoreGui = game:GetService("CoreGui")
local SoundService = game:GetService("SoundService")
local UserInputService = game:GetService("UserInputService")

-- SÃ³ ativa no Brookhaven
if game.PlaceId == 4924922222 then
    local gui = Instance.new("ScreenGui", CoreGui)
    gui.Name = "VoidReaperHub"

    local abrir = Instance.new("TextButton", gui)
    abrir.Size = UDim2.new(0, 120, 0, 30)
    abrir.Position = UDim2.new(0, 10, 0, 10)
    abrir.Text = "VoidReaper Hub"
    abrir.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    abrir.TextColor3 = Color3.fromRGB(255, 0, 0)
    abrir.Font = Enum.Font.GothamBold
    abrir.TextSize = 16

    local frame = Instance.new("Frame", gui)
    frame.Size = UDim2.new(0, 480, 0, 280)
    frame.Position = UDim2.new(0.5, -240, 0.5, -140)
    frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    frame.Visible = false
    frame.Active = true
    frame.Draggable = true

    local lista = Instance.new("ScrollingFrame", frame)
    lista.Size = UDim2.new(0, 160, 0, 240)
    lista.Position = UDim2.new(0, 10, 0, 10)
    lista.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    lista.ScrollBarThickness = 6

    local acoes = Instance.new("Frame", frame)
    acoes.Size = UDim2.new(0, 280, 0, 240)
    acoes.Position = UDim2.new(0, 190, 0, 10)
    acoes.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

    local selectedPlayer = nil
    local controlando = nil

    local function atualizarLista()
        lista:ClearAllChildren()
        local y = 0
        for _, p in ipairs(Players:GetPlayers()) do
            if p ~= LocalPlayer then
                local b = Instance.new("TextButton", lista)
                b.Size = UDim2.new(1, -10, 0, 25)
                b.Position = UDim2.new(0, 5, 0, y)
                b.Text = p.Name
                b.TextColor3 = Color3.fromRGB(255, 0, 0)
                b.BackgroundColor3 = Color3.fromRGB(50, 0, 0)
                b.Font = Enum.Font.Gotham
                b.TextSize = 14
                b.MouseButton1Click:Connect(function()
                    selectedPlayer = p
                    b.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
                end)
                y += 30
            end
        end
        lista.CanvasSize = UDim2.new(0, 0, 0, y)
    end

    atualizarLista()
    Players.PlayerAdded:Connect(function()
        wait(1)
        atualizarLista()
    end)

    local function botao(txt, y, func)
        local b = Instance.new("TextButton", acoes)
        b.Size = UDim2.new(1, -20, 0, 25)
        b.Position = UDim2.new(0, 10, 0, y)
        b.Text = txt
        b.TextColor3 = Color3.fromRGB(255, 0, 0)
        b.BackgroundColor3 = Color3.fromRGB(40, 0, 0)
        b.Font = Enum.Font.GothamBold
        b.TextSize = 14
        b.MouseButton1Click:Connect(func)
    end

    botao("Jaula", 0, function()
        if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local root = selectedPlayer.Character.HumanoidRootPart
            local base = Instance.new("Part", workspace)
            base.Size = Vector3.new(10, 1, 10)
            base.Position = Vector3.new(0, 5, 0)
            base.Anchored = true
            base.Color = Color3.fromRGB(60, 0, 80)
            local function parede(pos, size)
                local p = Instance.new("Part", workspace)
                p.Size = size
                p.Position = base.Position + pos
                p.Anchored = true
                p.Color = Color3.fromRGB(128, 0, 128)
                p.Material = Enum.Material.Neon
            end
            parede(Vector3.new(0, 3, -5), Vector3.new(10, 6, 1))
            parede(Vector3.new(0, 3, 5), Vector3.new(10, 6, 1))
            parede(Vector3.new(-5, 3, 0), Vector3.new(1, 6, 10))
            parede(Vector3.new(5, 3, 0), Vector3.new(1, 6, 10))
            parede(Vector3.new(0, 6, 0), Vector3.new(10, 1, 10))
            root.CFrame = CFrame.new(base.Position + Vector3.new(0, 2, 0))
        end
    end)

    botao("Teleportar", 30, function()
        if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
            LocalPlayer.Character.HumanoidRootPart.CFrame = selectedPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 2, 0)
        end
    end)

    botao("Desbanir da Casa", 60, function()
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("Model") and obj:FindFirstChild("BanList") then
                local banList = obj.BanList
                if banList:FindFirstChild(LocalPlayer.Name) then
                    banList[LocalPlayer.Name]:Destroy()
                end
            end
        end
    end)

    botao("Controlar Jogador", 90, function()
        if selectedPlayer then controlando = selectedPlayer end
    end)

    botao("Fling", 120, function()
        if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local v = Instance.new("BodyVelocity", selectedPlayer.Character.HumanoidRootPart)
            v.Velocity = Vector3.new(1000, 1000, 1000)
            v.MaxForce = Vector3.new(1e5, 1e5, 1e5)
            wait(0.5)
            v:Destroy()
        end
    end)

    botao("Som de Erro", 150, function()
        local s = Instance.new("Sound", SoundService)
        s.SoundId = "rbxassetid://911882310"
        s.Volume = 1
        s:Play()
    end)

    botao("Skybox Camisa FE", 180, function()
        local shirt = Instance.new("Shirt", LocalPlayer.Character)
        shirt.ShirtTemplate = "rbxassetid://17372543307" -- ID da camisa bugada que simula skybox
    end)

    UserInputService.InputBegan:Connect(function(input, gp)
        if controlando and input.KeyCode == Enum.KeyCode.Return and not gp then
            local chatGui = LocalPlayer:FindFirstChild("PlayerGui"):FindFirstChild("Chat")
            if chatGui then
                for _, obj in pairs(chatGui:GetDescendants()) do
                    if obj:IsA("TextBox") and obj.Text ~= "" then
                        local msg = Instance.new("Message", workspace)
                        msg.Text = controlando.Name .. ": " .. obj.Text
                        wait(3)
                        msg:Destroy()
                        break
                    end
                end
            end
        end
    end)

    abrir.MouseButton1Click:Connect(function()
        frame.Visible = not frame.Visible
    end)
end
