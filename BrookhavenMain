local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local player = Players.LocalPlayer
local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "VoidReaperHub"

local senha = "Vø!d6#X_Çørr0p!d0"

local function normalizar(texto)
	return texto:lower():gsub("ø", "o"):gsub("ç", "c"):gsub("!", ""):gsub("#", ""):gsub(" ", "")
end

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 300, 0, 150)
frame.Position = UDim2.new(0.5, -150, 0.5, -75)
frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)

local label = Instance.new("TextLabel", frame)
label.Size = UDim2.new(1, 0, 0, 30)
label.Text = "Digite a senha"
label.TextColor3 = Color3.fromRGB(255, 0, 0)
label.BackgroundTransparency = 1
label.Font = Enum.Font.Code
label.TextSize = 18

local input = Instance.new("TextBox", frame)
input.Size = UDim2.new(0.8, 0, 0, 30)
input.Position = UDim2.new(0.1, 0, 0, 40)
input.PlaceholderText = "Senha..."
input.TextColor3 = Color3.fromRGB(0, 255, 0)
input.BackgroundColor3 = Color3.fromRGB(30, 0, 0)
input.Font = Enum.Font.Code
input.TextSize = 16

local confirmar = Instance.new("TextButton", frame)
confirmar.Size = UDim2.new(0.5, 0, 0, 30)
confirmar.Position = UDim2.new(0.25, 0, 0, 90)
confirmar.Text = "Confirmar"
confirmar.BackgroundColor3 = Color3.fromRGB(30, 0, 0)
confirmar.TextColor3 = Color3.fromRGB(0, 255, 0)
confirmar.Font = Enum.Font.Code
confirmar.TextSize = 16

confirmar.MouseButton1Click:Connect(function()
	if normalizar(input.Text) == normalizar(senha) then
		frame:Destroy()

		local char = player.Character or player.CharacterAdded:Wait()
		local hum = char:FindFirstChildOfClass("Humanoid")
		local root = char:FindFirstChild("HumanoidRootPart")
		if hum then
			hum.WalkSpeed = 100
			hum.JumpPower = 150
			hum.MaxHealth = math.huge
			hum.Health = math.huge
		end

		local aura = Instance.new("Part", char)
		aura.Size = Vector3.new(8, 8, 8)
		aura.Shape = Enum.PartType.Ball
		aura.Material = Enum.Material.Neon
		aura.Color = Color3.fromRGB(0, 255, 255)
		aura.Anchored = false
		aura.CanCollide = false
		aura.Transparency = 0.4
		local weld = Instance.new("WeldConstraint", aura)
		weld.Part0 = aura
		weld.Part1 = root

		local light = Instance.new("PointLight", root)
		light.Color = Color3.fromRGB(0, 255, 255)
		light.Range = 30
		light.Brightness = 5

		local particles = Instance.new("ParticleEmitter", root)
		particles.Texture = "rbxassetid://243098098"
		particles.Rate = 50
		particles.Lifetime = NumberRange.new(2)
		particles.Speed = NumberRange.new(1)
		particles.Size = NumberSequence.new(3)
		particles.Color = ColorSequence.new(Color3.fromRGB(0, 255, 255))
		particles.LightEmission = 0.5

		for _, v in pairs(Lighting:GetChildren()) do
			if v:IsA("Sky") then v:Destroy() end
		end
		local sky = Instance.new("Sky", Lighting)
		sky.SkyboxBk = "rbxassetid://0"
		sky.SkyboxDn = "rbxassetid://0"
		sky.SkyboxFt = "rbxassetid://0"
		sky.SkyboxLf = "rbxassetid://0"
		sky.SkyboxRt = "rbxassetid://0"
		sky.SkyboxUp = "rbxassetid://0"
		Lighting.Ambient = Color3.fromRGB(255, 0, 0)
		Lighting.OutdoorAmbient = Color3.fromRGB(50, 0, 0)
		Lighting.FogColor = Color3.fromRGB(255, 255, 255)
		Lighting.FogEnd = 500
		Lighting.FogStart = 0
		Lighting.ClockTime = 0
		Lighting.Brightness = 1
	end
end)
