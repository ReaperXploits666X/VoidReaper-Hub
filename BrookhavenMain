--[[
    VOIDREAPER_HAVOC666X // V72 THE APOCALYPSE
    REPOSIT√ìRIO: ReaperXploits666X/VoidReaper-Hub
    STATUS: OMNIPOTENTE (GOD-MODE / CAOS / ANTI-HACKER)
]]

local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local RS = game:GetService("RunService")
local Lighting = game:GetService("Lighting")
local StarterGui = game:GetService("StarterGui")
local Camera = workspace.CurrentCamera

-- üîê CONFIGURA√á√ÉO DE ELITE
local Owner = "VoidReaper_Havoc666X"
local Whitelist = {
    ["VoidReaper_Havoc666X"] = true,
    ["tequinhasamu"] = true,
    ["Luffy_DIABO"] = true,
    ["rafisamumu"] = true
}

-- üõ†Ô∏è ESTADOS
local Noclipping = false
local GhostActive = false
local EarthQuake = false
local AutoBanHacks = false

-- üìã RASTREIO DE USU√ÅRIOS DO SEU SCRIPT (BACKDOOR)
_G.ReaperUsers = _G.ReaperUsers or {}
if not table.find(_G.ReaperUsers, LP.Name) then table.insert(_G.ReaperUsers, LP.Name) end

-- üåÄ SISTEMA DE NOTIFICA√á√ÉO (CHAT LOCAL)
local function Notify(text)
    StarterGui:SetCore("ChatMakeSystemMessage", {
        Text = "{VOID-REAPER}: " .. text;
        Color = Color3.fromRGB(255, 0, 0);
        Font = Enum.Font.SourceSansBold;
        TextSize = 20;
    })
end

-- üåÄ MOTOR DE BANIMENTO (F√çSICA DE 1 BILH√ÉO)
local function Obliterate(target)
    if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
        local hrp = LP.Character.HumanoidRootPart
        local thrp = target.Character.HumanoidRootPart
        local oldPos = hrp.CFrame
        
        local bv = Instance.new("BodyVelocity", hrp)
        bv.MaxForce = Vector3.new(1e9, 1e9, 1e9)
        bv.Velocity = Vector3.new(0, 1e8, 0)
        
        local start = tick()
        while tick() - start < 0.2 do
            hrp.CFrame = thrp.CFrame
            task.wait()
        end
        
        bv:Destroy()
        hrp.CFrame = oldPos
    end
end

-- üåÄ SCANNER DE HACKERS RIVAIS
local function ScanAndBan()
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= LP and not Whitelist[p.Name] then
            local isHacker = false
            -- Detec√ß√£o por GUI Rival
            if p:FindFirstChild("PlayerGui") and (p.PlayerGui:FindFirstChild("93") or p.PlayerGui:FindFirstChild("Coquette") or p.PlayerGui:FindFirstChild("Cartola") or p.PlayerGui:FindFirstChild("Pendulum")) then
                isHacker = true
            end
            -- Detec√ß√£o por Velocidade (Acima de 30)
            if p.Character and p.Character:FindFirstChild("Humanoid") and p.Character.Humanoid.WalkSpeed > 30 then
                isHacker = true
            end
            
            if isHacker then
                Notify("Hacker Detectado: " .. p.Name .. " - BANINDO...")
                Obliterate(p)
            end
        end
    end
end

-- üåÄ LOOP DE ATUALIZA√á√ÉO
RS.Stepped:Connect(function()
    if LP.Character and Noclipping then
        for _, v in pairs(LP.Character:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide = false end end
    end
    if AutoBanHacks then ScanAndBan() end
    if EarthQuake then
        Camera.CoordinateFrame = Camera.CoordinateFrame * CFrame.fromEulerAnglesXYZ(math.random(-5,5)/100, math.random(-5,5)/100, math.random(-5,5)/100)
    end
end)

-- --- ‚å®Ô∏è CONSOLE DE COMANDOS SUPREMOS ---

LP.Chatted:Connect(function(msg)
    if LP.Name ~= Owner then return end -- S√ì VOC√ä MANDA
    
    local args = msg:split(" ")
    local cmd = args[1]:lower()
    local arg2 = args[2]

    -- [ ;list ] - A LISTA COMPLETA SEM REMO√á√ïES
    if cmd == ";list" then
        Notify("--- üíÄ COMANDOS DE BANIMENTO üíÄ ---")
        Notify(";kick [nick] - Bane por F√≠sica Suprema")
        Notify(";banall - Limpa o servidor (The Purge)")
        Notify(";banhacks - Bane hackers rivais agora")
        Notify(";autoban - Ativa/Desativa Ban Autom√°tico de hacks")
        Notify(";banusers - Bane todos que executaram SEU script")
        Notify("--- üëª COMANDOS DE ENTIDADE ---")
        Notify(";invisible / ;uninv - Ficar Invis√≠vel")
        Notify(";zika - Full Mode (Invis/Speed/Noclip/ESP)")
        Notify(";noclip / ;clip - Atravessar Paredes")
        Notify(";ghostwalk - Mata quem voc√™ encostar")
        Notify("--- üå™Ô∏è COMANDOS DE CAOS ---")
        Notify(";voidsky / ;hellsky - Muda o C√©u")
        Notify(";unhouse - Destr√≥i barreiras de casas")
        Notify(";scare [nick] - Jumpscare (Grito)")
        Notify(";earthquake - Terremoto na tela")
        Notify(";bring all / ;bring [nick] - Puxa alvos")
        Notify(";freezeall - Congela os mortais")
    end

    -- [ COMANDOS DE BAN ]
    if (cmd == ";kick" or cmd == ";ban") and arg2 then
        local t = nil
        for _, p in pairs(Players:GetPlayers()) do if p.Name:lower():sub(1,#arg2) == arg2:lower() then t = p end end
        if t then Obliterate(t) Notify(t.Name .. " EXPULSO.") end
    end

    if cmd == ";banall" then
        Notify("PURGUE EM ANDAMENTO...")
        for _, p in pairs(Players:GetPlayers()) do if p ~= LP and not Whitelist[p.Name] then Obliterate(p) task.wait(0.05) end end
    end

    if cmd == ";banhacks" then ScanAndBan() end
    
    if cmd == ";autoban" then
        AutoBanHacks = not AutoBanHacks
        Notify("AUTO-BAN HACKERS: " .. (AutoBanHacks and "LIGADO" or "DESLIGADO"))
    end

    if cmd == ";banusers" then
        for _, name in pairs(_G.ReaperUsers) do
            local p = Players:FindFirstChild(name)
            if p and p ~= LP and not Whitelist[p.Name] then Obliterate(p) end
        end
        Notify("USU√ÅRIOS DO SCRIPT ELIMINADOS.")
    end

    -- [ ENTIDADE ]
    if cmd == ";invisible" then
        for _, v in pairs(LP.Character:GetDescendants()) do if v:IsA("BasePart") or v:IsA("Decal") then v.Transparency = 1 end end
    elseif cmd == ";uninv" then
        for _, v in pairs(LP.Character:GetDescendants()) do if v:IsA("BasePart") or v:IsA("Decal") then v.Transparency = 0 end end
    elseif cmd == ";zika" then
        Noclipping = true
        LP.Character.Humanoid.WalkSpeed = 250
        for _, p in pairs(Players:GetPlayers()) do if p ~= LP and p.Character then Instance.new("Highlight", p.Character).FillColor = Color3.new(1,0,0) end end
    end

    -- [ CAOS ]
    if cmd == ";voidsky" then
        for _, v in pairs(Lighting:GetChildren()) do if v:IsA("Sky") then v:Destroy() end end
        local s = Instance.new("Sky", Lighting) s.SkyboxBk = "rbxassetid://159067838" s.SkyboxDn = "rbxassetid://159067838"
        s.SkyboxFt = "rbxassetid://159067838" s.SkyboxLf = "rbxassetid://159067838" s.SkyboxRt = "rbxassetid://159067838" s.SkyboxUp = "rbxassetid://159067838"
        Lighting.ClockTime = 0
    end

    if cmd == ";unhouse" then
        for _, v in pairs(workspace:GetDescendants()) do if v.Name == "BanBlock" or v.Name == "Barrier" or v.Name == "Door" then v:Destroy() end end
    end

    if cmd == ";scare" and arg2 then
        local t = nil
        for _, p in pairs(Players:GetPlayers()) do if p.Name:lower():sub(1,#arg2) == arg2:lower() then t = p end end
        if t then
            local s = Instance.new("Sound", t.Character.HumanoidRootPart) s.SoundId = "rbxassetid://4835664244" s.Volume = 10 s:Play()
            LP.Character.HumanoidRootPart.CFrame = t.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-2)
        end
    end

    if cmd == ";earthquake" then EarthQuake = not EarthQuake end
    
    if cmd == ";noclip" then Noclipping = true elseif cmd == ";clip" then Noclipping = false end

    if cmd == ";bring" and arg2 then
        if arg2 == "all" then
            for _, p in pairs(Players:GetPlayers()) do if p ~= LP and not Whitelist[p.Name] and p.Character then p.Character.HumanoidRootPart.CFrame = LP.Character.HumanoidRootPart.CFrame end end
        end
    end
end)

Notify("VOID-REAPER HUB V72 ONLINE.")
Notify("USE ;LIST PARA DOMINAR O HEAVEN.")

