--[[
    VOIDREAPER_HAVOC666X // ULTIMATE OMNIPOTENCE V70
    REPOSIT√ìRIO: ReaperXploits666X/VoidReaper-Hub
    STATUS: GOD-MODE / CAOS / ANTI-HACKER
]]

local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local RS = game:GetService("RunService")
local Lighting = game:GetService("Lighting")
local StarterGui = game:GetService("StarterGui")
local Camera = workspace.CurrentCamera

-- üîê WHITELIST (IMUNIDADE TOTAL)
local Auth = {
    ["VoidReaper_Havoc666X"] = true,
    ["tequinhasamu"] = true,
    ["Luffy_DIABO"] = true,
    ["rafisamumu"] = true
}

-- üõ†Ô∏è VARI√ÅVEIS DE CONTROLE
local Noclipping = false
local GhostActive = false
local FastCycle = false
local EarthQuake = false

-- üåÄ SISTEMA DE NOTIFICA√á√ÉO (APARECE NO CHAT S√ì PRA VOC√ä)
local function Notify(text)
    StarterGui:SetCore("ChatMakeSystemMessage", {
        Text = "{VOID-REAPER}: " .. text;
        Color = Color3.fromRGB(255, 255, 0); -- Amarelo para destaque
        Font = Enum.Font.SourceSansBold;
        TextSize = 20;
    })
end

-- üåÄ BUSCA DE ALVO
local function GetPlayer(name)
    name = name:lower()
    for _, p in pairs(Players:GetPlayers()) do
        if p.Name:lower():sub(1, #name) == name or p.DisplayName:lower():sub(1, #name) == name then
            return p
        end
    end
end

-- üåÄ MANIPULA√á√ÉO DE SKYBOX
local function SetSky(id)
    for _, v in pairs(Lighting:GetChildren()) do if v:IsA("Sky") then v:Destroy() end end
    local s = Instance.new("Sky", Lighting)
    s.SkyboxBk = "rbxassetid://"..id s.SkyboxDn = "rbxassetid://"..id s.SkyboxFt = "rbxassetid://"..id
    s.SkyboxLf = "rbxassetid://"..id s.SkyboxRt = "rbxassetid://"..id s.SkyboxUp = "rbxassetid://"..id
end

-- üåÄ MOTORES DE F√çSICA E LOOP
RS.Stepped:Connect(function()
    if LP.Character then
        if Noclipping then
            for _, v in pairs(LP.Character:GetDescendants()) do if v:IsA("BasePart") then v.CanCollide = false end end
        end
        if GhostActive then
            for _, p in pairs(Players:GetPlayers()) do
                if p ~= LP and not Auth[p.Name] and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                    if (LP.Character.HumanoidRootPart.Position - p.Character.HumanoidRootPart.Position).Magnitude < 4 then
                        local bv = Instance.new("BodyVelocity", p.Character.HumanoidRootPart)
                        bv.Velocity = Vector3.new(0, 500000, 0) task.wait(0.1) bv:Destroy()
                    end
                end
            end
        end
    end
    if EarthQuake then
        Camera.CoordinateFrame = Camera.CoordinateFrame * CFrame.fromEulerAnglesXYZ(math.random(-10,10)/100, math.random(-10,10)/100, math.random(-10,10)/100)
    end
end)

-- --- ‚å®Ô∏è CONSOLE DE COMANDOS (THE OVERLORD) ---

LP.Chatted:Connect(function(msg)
    local args = msg:split(" ")
    local cmd = args[1]:lower()
    local arg2 = args[2]

    -- [ ;list ] - A LISTA COMPLETA DE TODOS OS PODERES
    if cmd == ";list" then
        Notify("--- üíÄ LISTA SUPREMA VOID-REAPER üíÄ ---")
        Notify(";kick/ ;ban [nick] - Elimina Player do Servidor")
        Notify(";banall - Limpa todos (Exceto Whitelist)")
        Notify(";banscripts - Ca√ßa e elimina usu√°rios de outros scripts")
        Notify(";verifique - Identifica hackers rivais no mapa")
        Notify(";invisible / ;uninv - Modo Fantasma (Desaparecer)")
        Notify(";zika - Ativa Speed, Noclip, ESP e Invis")
        Notify(";ghostwalk - Mata quem voc√™ encostar")
        Notify(";scare [nick] - Jumpscare (Grito Agudo)")
        Notify(";voidsky / ;hellsky / ;glitchsky - Muda o C√©u")
        Notify(";fastcycle - Dia e Noite girando ultra r√°pido")
        Notify(";unhouse - Destr√≥i portas e barreiras de casas")
        Notify(";freezeall - Paralisa todos os jogadores")
        Notify(";earthquake - Terremoto na tela de todos")
        Notify(";bring all / ;bring [nick] - Puxa as v√≠timas")
        Notify(";nightmare - Escurid√£o total e n√©voa")
        Notify(";noclip / ;clip - Atravessar paredes")
    end

    -- [ BANIMENTOS E KICKS ]
    if (cmd == ";kick" or cmd == ";ban" or cmd == ";kill") and arg2 then
        local t = GetPlayer(arg2)
        if t and t.Character then
            local bv = Instance.new("BodyVelocity", LP.Character.HumanoidRootPart)
            bv.Velocity = Vector3.new(0, 1e7, 0)
            LP.Character.HumanoidRootPart.CFrame = t.Character.HumanoidRootPart.CFrame
            task.wait(0.1) bv:Destroy()
            Notify(t.Name .. " DELETADO.")
        end
    end

    -- [ BANALL / PURGE ]
    if cmd == ";banall" then
        Notify("PURGE ATIVADO. LIMPANDO SERVIDOR...")
        for _, p in pairs(Players:GetPlayers()) do
            if p ~= LP and not Auth[p.Name] and p.Character then
                local bv = Instance.new("BodyVelocity", LP.Character.HumanoidRootPart)
                bv.Velocity = Vector3.new(0, 1e7, 0)
                LP.Character.HumanoidRootPart.CFrame = p.Character.HumanoidRootPart.CFrame
                task.wait(0.05) bv:Destroy()
            end
        end
    end

    -- [ CA√áA-HACKERS ]
    if cmd == ";verifique" or cmd == ";banscripts" then
        for _, p in pairs(Players:GetPlayers()) do
            if p:FindFirstChild("PlayerGui") and (p.PlayerGui:FindFirstChild("93") or p.PlayerGui:FindFirstChild("Coquette") or p.PlayerGui:FindFirstChild("Cartola")) then
                Notify("HACKER DETECTADO: " .. p.Name)
                Instance.new("Highlight", p.Character).FillColor = Color3.new(1,0,0)
                if cmd == ";banscripts" then 
                    LP.Character.HumanoidRootPart.CFrame = p.Character.HumanoidRootPart.CFrame
                    local bv = Instance.new("BodyVelocity", LP.Character.HumanoidRootPart)
                    bv.Velocity = Vector3.new(0, 1e7, 0) task.wait(0.1) bv:Destroy()
                end
            end
        end
    end

    -- [ MODOS DE ENTIDADE ]
    if cmd == ";invisible" then
        for _, v in pairs(LP.Character:GetDescendants()) do if v:IsA("BasePart") or v:IsA("Decal") then v.Transparency = 1 end end
        Notify("INVIS√çVEL.")
    elseif cmd == ";uninv" then
        for _, v in pairs(LP.Character:GetDescendants()) do if v:IsA("BasePart") or v:IsA("Decal") then v.Transparency = 0 end end
        Notify("VIS√çVEL.")
    elseif cmd == ";zika" then
        Noclipping = true
        LP.Character.Humanoid.WalkSpeed = 250
        for _, p in pairs(Players:GetPlayers()) do if p ~= LP and p.Character then Instance.new("Highlight", p.Character).FillColor = Color3.new(1,0,0) end end
        Notify("MODO ZIKA ATIVADO.")
    end

    -- [ MANIPULA√á√ÉO DE MUNDO ]
    if cmd == ";voidsky" then SetSky("159067838") Lighting.ClockTime = 0
    elseif cmd == ";hellsky" then SetSky("600886096") Lighting.ClockTime = 0
    elseif cmd == ";glitchsky" then SetSky("270031043")
    elseif cmd == ";unhouse" then
        for _, v in pairs(workspace:GetDescendants()) do if v.Name == "BanBlock" or v.Name == "Barrier" or v.Name == "Door" then v:Destroy() end end
        Notify("BARREIRAS DELETADAS.")
    elseif cmd == ";earthquake" then
        EarthQuake = not EarthQuake
        Notify("TERREMOTO: " .. tostring(EarthQuake))
    end

    -- [ COMANDOS DE TERROR ]
    if cmd == ";scare" and arg2 then
        local t = GetPlayer(arg2)
        if t then
            local s = Instance.new("Sound", t.Character.HumanoidRootPart)
            s.SoundId = "rbxassetid://4835664244" s.Volume = 10 s:Play()
            LP.Character.HumanoidRootPart.CFrame = t.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-2)
        end
    elseif cmd == ";nightmare" then
        Lighting.FogEnd = 50 Lighting.FogColor = Color3.new(0,0,0) Lighting.ClockTime = 0
    end
end)

Notify("VOID-REAPER HUB V70: PRONTO PARA O CAOS.")
print("HUB CARREGADO. USE ;LIST NO CHAT.")
